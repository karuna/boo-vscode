{
    "name": "boo",
    "displayName": "Boo Syntax",
    "description": "Boo syntax highlighting",
    "version": "0.0.1",
    "publisher": "karunamurti",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "license": "MIT",
    "keywords": [
        "boo"
    ],
    "contributes": {
        "languages": [{
            "id": "boo",
            "aliases": ["boo", "boo"],
            "extensions": [".boo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "boo",
            "scopeName": "source.boo",
            "path": "./syntaxes/boo.tmLanguage"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/karuna/boo-vscode.git"
    },
    "galleryBanner": {
        "color": "#888888",
        "theme": "dark"
    },
    "homepage": "https://github.com/karuna/boo-vscode",
    "bugs": {
        "url": "https://github.com/karuna/boo-vscode/issues",
        "email": "karuna.murti@gmail.com"
    },
    "icon": "images/logo.png"
}